                <discovery_rule>
                    <uuid>1ac24ad30f5f48fb9d2df645da077ff1</uuid>
                    <name>Filesystem Discovery</name>
                    <type>SIMPLE</type>
                    <key>vpoller[&quot;vm.disk.discover&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;null&quot;, &quot;null&quot;]</key>
                    <delay>3600</delay>
                    <lifetime>7d</lifetime>
                    <description>Discovery of filesystems on a VMware Virtual Machine</description>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>f8d32c72387d4270853ca94308929bae</uuid>
                            <name>Capacity of {#VSPHERE.VM.DISKPATH} filesystem</name>
                            <type>SIMPLE</type>
                            <key>vpoller[&quot;vm.disk.get&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;capacity&quot;, {#VSPHERE.VM.DISKPATH}]</key>
                            <delay>300</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <units>B</units>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Filesystem</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>6134495e0b61432db95ec7bf5ab56f39</uuid>
                            <name>Free space of {#VSPHERE.VM.DISKPATH} filesystem</name>
                            <type>SIMPLE</type>
                            <key>vpoller[&quot;vm.disk.get&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;freeSpace&quot;, {#VSPHERE.VM.DISKPATH}]</key>
                            <delay>300</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <units>B</units>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Filesystem</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>6c6cf57cb08641e2a0e4e412e0867c4a</uuid>
                            <name>Free space of {#VSPHERE.VM.DISKPATH} filesystem (percentage)</name>
                            <type>CALCULATED</type>
                            <key>vpoller[&quot;vm.disk.get&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;freeSpacePercentage&quot;, {#VSPHERE.VM.DISKPATH}]</key>
                            <delay>300</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <value_type>FLOAT</value_type>
                            <params>last(//vpoller[&quot;vm.disk.get&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;freeSpace&quot;, {#VSPHERE.VM.DISKPATH}]) / last(//vpoller[&quot;vm.disk.get&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;capacity&quot;, {#VSPHERE.VM.DISKPATH}]) * 100</params>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Filesystem</value>
                                </tag>
                            </tags>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <uuid>a3f65e68de6a418d90617439cc4e1bad</uuid>
                                    <expression>min(/Template VMware vSphere Virtual Machine - vPoller Native/vpoller[&quot;vm.disk.get&quot;, &quot;{$VSPHERE.HOST}&quot;, &quot;{HOST.HOST}&quot;, &quot;freeSpacePercentage&quot;, {#VSPHERE.VM.DISKPATH}],5s)&lt;5</expression>
                                    <name>Less than 5% of free space on {#VSPHERE.VM.DISKPATH} at {HOST.NAME}</name>
                                    <priority>HIGH</priority>
                                    <description>Less than 5% of free space on {#VSPHERE.VM.DISKPATH} at {HOST.NAME}

Virtual Machine: {HOST.NAME}
Free space of {#VSPHERE.VM.DISKPATH}: {ITEM.LASTVALUE}%
vSphere Host: {$VSPHERE.HOST}</description>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
